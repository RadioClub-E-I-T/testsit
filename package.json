{
  "name": "quiz-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "super": "supervisor ./bin/www",
    "test": "NODE_ENV=testing mocha -R spec",
    "migrate": "sequelize db:migrate --url sqlite://$(pwd)/quiz.sqlite",
    "seed": "sequelize db:seed:all --url sqlite://$(pwd)/quiz.sqlite",
    "migrate_win": "sequelize db:migrate --url sqlite://%cd%/quiz.sqlite",
    "seed_win": "sequelize db:seed:all --url sqlite://%cd%/quiz.sqlite",
    "migrate_env": "sequelize db:migrate --url $DATABASE_URL",
    "seed_env": "sequelize db:seed:all --url $DATABASE_URL"
  },
  "engines": {
    "node": "10.19.0",
    "npm": "6.14.4"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bulma-start": "0.0.3",
    "chart.js": "^2.8.0",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-flash": "0.0.2",
    "express-http-to-https": "^1.1.4",
    "express-partials": "^0.3.0",
    "express-session": "^1.16.1",
    "handlebars": "^4.7.6",
    "http-errors": "~1.6.2",
    "mathjax-node": "^2.1.1",
    "method-override": "^3.0.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.19.0",
    "pg": "^7.11.0",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.2.0",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "bulma": "^0.7.5",
    "node-sass": "^4.14.1",
    "sqlite3": "^4.2.0"
  }
}
